+   "metadata": {},
+   "outputs": [],
+   "source": [
+    "# Create pulse report\n",
+    "report = PulseReport(\n",
+    "    result.final_amps[0],\n",
+    "    times=np.linspace(0, gate_time, n_timeslots),\n",
