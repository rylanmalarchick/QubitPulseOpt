+    "fig = report.generate_full_report()\n",
+    "plt.show()\n",
+    "\n",
+    "# Export\n",
+    "report.export_metrics_table('x_gate_metrics.tex', format='latex')\n",
+    "report.export_metrics_table('x_gate_metrics.json', format='json')\n",
+    "\n",
+    "print(\"✓ Reports generated and exported\")"
+   ]
+  },
+  {
+   "cell_type": "markdown",
+   "metadata": {},
+   "source": [
